# Pypboy Configuration
# Copy this file to .env and set your values

# Your GPS coordinates (longitude, latitude)
# Example: Santa Clarita, CA
MAP_LONGITUDE=-118.5723894
MAP_LATITUDE=34.3917171

# Touch device path for Raspberry Pi (default: /dev/input/event2)
# TOUCH_DEVICE=/dev/input/event2

# Load map from cache instead of internet (true/false)
# LOAD_CACHED_MAP=false
